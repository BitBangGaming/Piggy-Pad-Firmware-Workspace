#include "stm32f4xx_hal.h"

// LED = PC13, bus AHB1EN bit0
#define	BLUE_LED_PIN_HAL	GPIO_PIN_13

int main(void)
{
	HAL_Init();

	/* Quick test to make sure everything compiles okay in STM32CubeIDE */

	// Configure PC13 as output pin (blue LED on blackpill)

	// Enable the GPIO clock
	__HAL_RCC_GPIOC_CLK_ENABLE();

	GPIO_InitTypeDef GPIO_InitStruct = {0};

	GPIO_InitStruct.Pin = BLUE_LED_PIN_HAL;


    while(1)
    {

    }
}
