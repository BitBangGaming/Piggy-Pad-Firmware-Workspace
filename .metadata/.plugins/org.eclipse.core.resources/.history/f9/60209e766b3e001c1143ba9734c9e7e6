#ifndef IO_MAPPING_STM32F411CE_BLACKPILL_WEACTSTUDIO_V3_0_H_
#define IO_MAPPING_STM32F411CE_BLACKPILL_WEACTSTUDIO_V3_0_H_

#define COMMS_TX_EXPANSION1_PIN			(0U)
#define COMMS_TX_EXPANSION1_PIN_HAL		(GPIO_PIN_0)
#define COMMS_TX_EXPANSION1_PORT 		(GPIOA)
#define COMMS_TX_EXPANSION1_BIT 		(1 << COMMS_TX_EXPANSION1_PIN)

#define COMMS_RX_EXPANSION1_PIN			(1U)
#define COMMS_RX_EXPANSION1_PIN_HAL		(GPIO_PIN_1)
#define COMMS_RX_EXPANSION1_PORT 		(GPIOA)
#define COMMS_RX_EXPANSION1_BIT 		(1 << COMMS_RX_EXPANSION1_PIN)

#define COMMS_TX_EXPANSION2_PIN			(2U)
#define COMMS_TX_EXPANSION2_PIN_HAL		(GPIO_PIN_2)
#define COMMS_TX_EXPANSION2_PORT 		(GPIOA)
#define COMMS_TX_EXPANSION2_BIT 		(1 << COMMS_TX_EXPANSION2_PIN)

#define COMMS_RX_EXPANSION2_PIN			(3U)
#define COMMS_RX_EXPANSION2_PIN_HAL		(GPIO_PIN_3)
#define COMMS_RX_EXPANSION2_PORT 		(GPIOA)
#define COMMS_RX_EXPANSION2_BIT 		(1 << COMMS_RX_EXPANSION2_PIN)

#endif
